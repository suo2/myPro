package com.pinnet.chargerapp.utils;

import com.google.gson.Gson;

/**
 * @author P00558
 * @date 2018/5/24
 */

public class GsonUtils {
    public Gson mGson;

    public GsonUtils() {
        mGson = new Gson();
    }

    public static GsonUtils getInstance() {
        return GsonUtilHolder.instance;
    }

    public static class GsonUtilHolder {
        public static GsonUtils instance = new GsonUtils();
    }
}
