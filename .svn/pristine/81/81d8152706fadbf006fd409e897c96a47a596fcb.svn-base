<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical"
    android:padding="@dimen/main_padding_1x">

    <TextView
        android:id="@+id/tv_charge_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/main_margin_1x"
        android:gravity="center_vertical"
        android:text="快充电桩5"
        android:textColor="@color/text_color_333333"
        android:textSize="@dimen/textsize_12sp" />

    <ImageView
        android:id="@+id/iv_tips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/main_margin_1x"
        android:visibility="gone"
        android:layout_toRightOf="@+id/tv_charge_name"
        android:src="@drawable/charger_icon_question" />

    <TextView
        android:id="@+id/tv_manager_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_charge_name"
        android:background="@drawable/charger_muzzle_status_free_bg"
        android:gravity="center"
        android:paddingBottom="@dimen/main_padding_0.5x"
        android:paddingLeft="@dimen/main_padding_1.5x"
        android:paddingRight="@dimen/main_padding_1.5x"
        android:paddingTop="@dimen/main_padding_0.5x"
        android:text="@string/main_invate_string"
        android:textColor="@color/text_color_ffa415"
        android:textSize="@dimen/textsize_10sp" />
</RelativeLayout>

